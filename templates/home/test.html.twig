            // affix menu bar
            $('#nav').affix({
                offset: {
                    top: $('header.logo-header').height()
                }
            }).on('affix.bs.affix', function () {
                $('body').addClass('affixed');
                $('.fastsearch').hide();
            }).on('affix-top.bs.affix', function () {
                $('body').removeClass('affixed');
            });

            // unfocus search field
            $(window).scroll(function(){
                if($(this).scrollTop()>= 1){
                    $('[name="q"]').blur();
                }
            });

            // check if affix after reload
            if($('.navigation-header').hasClass('affix')) {
                $('body').addClass('affixed');
            }

            /**
             * set menu position left|right based on window size
             *
             * @param  {object}  windowWidth
             * @param  {integer} size
             * @return {string}
             */
            function setMenuPosition(windowWidth, size = 767)
            {
                return (windowWidth <= size) ? 'position-left' : 'position-right';
            }

            // set default position for slide-in effect
            var position = setMenuPosition($(window).width());

            $("#menu").mmenu({
                // navbars: [{
                // 	content: [ 'searchfield' ]
                // }],
                "extensions": [
                    position
                ],
                "pageScroll": true,
                wrappers: [ 'bootstrap4' ]}, {
            });
            $(".mh-head.mm-sticky").mhead({
                scroll: {
                    hide: 200
                }
            });
            $(".mh-head:not(.mm-sticky)").mhead({
                scroll: false
            });
        });

    </script>


