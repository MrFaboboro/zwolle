{% block navbar %}
  <nav class="navbar navbar-expand-md navbar-light fixed-top bg-light">
    <a class="navbar-brand" href="{{ path('home') }}">
      <img src="../../assets/images/logo.png" >
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarCollapse">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item">
          <a class="nav-link" href="{{ path('home') }}">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">{% if is_granted('IS_AUTHENTICATED_FULLY') %}
              <p>Welkom, {{ app.user.username }}</p>
            {% endif %}</a>
        </li>
      </ul>
    </div>
  </nav>
  <header>
    <div id="carouselExampleIndicators" class="carousel slide header-image" data-ride="carousel" >
      <div class="carousel-inner" role="listbox">
        <!-- Slide One - Set the background image for this slide in the line below -->
        <div class="carousel-item active" style="background-image: url('../../assets/images/zwolle_1.jpg')">
          <div class="dark-layer"></div>
        </div>
        <!-- Slide Two - Set the background image for this slide in the line below -->
        <div class="carousel-item" style="background-image: url('../../assets/images/zwolle_2.jpg')">
          <div class="dark-layer"></div>
        </div>
        <!-- Slide Three - Set the background image for this slide in the line below -->
        <div class="carousel-item" style="background-image: url('../../assets/images/zwolle_3.jpg')">
          <div class="dark-layer"></div>
        </div>
        <div class="container">
          <div class="search">
            {{ form_start(form) }}

            <div class="inner-form">
              <div class="input-field first-wrap search-text">
                {{ form_row(form.Zoek) }}
              </div>
              <div class="input-field third-wrap">
                <input type="submit" class="btn-search" type="button" />
              </div>
            </div>
            {{ form_end(form) }}
          </div>
        </div>
      </div>
    </div>
  </header>
{% endblock %}

{% block body %}{% endblock %}

